<Application x:Class="WpfComponentApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfComponentApp"
             StartupUri="pack://application:,,,/WpfComponentApp;component/UI/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Ame.Themes.Material;component/Styles/NativeControls.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Ame.Themes.Material;component/Styles/ExtendedToolkitControls.xaml" />

                <ResourceDictionary Source="pack://application:,,,/Ame.Themes.Material;component/Styles/Core/Effects.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Ame.Themes.Material;component/Styles/Core/Fonts.xaml" />
                <!--<ResourceDictionary Source="pack://application:,,,/Ame.Themes.Material;component/Styles/Core/LightPalette.xaml" />-->
                <ResourceDictionary Source="pack://application:,,,/Ame.Themes.Material;component/Styles/Core/LightPalette2.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
